#!/usr/bin/perl -w

use warnings;
use strict;
use RennPoints::ClubRegistration;
use Data::Dumper;

my $id = shift;
my $DEBUG = shift || 0;

my $clubreg = RennPoints::ClubRegistration->new( debug => $DEBUG );
my @participants = $clubreg->participants( $id );

if ( @participants ) {
    print Dumper( \@participants );
}
