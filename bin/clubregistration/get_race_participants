#!/usr/bin/perl -w

use warnings;
use strict;
use RennPoints::ClubRegistration::Race;
use Data::Dumper;

my $id = shift;

my $clubreg = RennPoints::ClubRegistration::Race->new( id => $id );

print $clubreg->content;
exit;
my $participants = $clubreg->participants;

if ( @$participants ) {
    print Dumper( $participants );
}
