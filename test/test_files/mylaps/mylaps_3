<script>
    (function(i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r;
        i[r] = i[r] ||
            function() {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
        a = s.createElement(o),
            m = s.getElementsByTagName(o)[0];
        a.async = 1;
        a.src = g;
        m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-108462-6', 'auto');
    //ga('send', 'pageview');
    var cookie = new MyCookie();
    var selectedSport = cookie.readCookie("home-event-type");

    ga('send',
        {
            hitType: 'event',
            eventCategory: 'User ID',
            eventAction: 'Sport',
            eventLabel: selectedSport || "all"
        });

    $(".all-tab,.mx-tab,.karting-tab,.car-tab,.rc-tab").on('click',
        function() {
            var selectedSport = $(this).find("a").data('sport-type');
            ga('send',
                {
                    hitType: 'event',
                    eventCategory: 'User ID',
                    eventAction: 'Filter Sport',
                    eventLabel: selectedSport || "all"
                });
        });
    $(".allresults-tab, .byclass-tab, .lapchartTab, .compare-tab").on('click',
        function() {
            var mode = $(this).attr("aria-controls");
            ga('send',
                {
                    hitType: 'event',
                    eventCategory: 'Event Results',
                    eventAction: 'Session Tabs',
                    eventLabel: mode || "all"
                });
        });
</script>
