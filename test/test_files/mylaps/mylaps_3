<script src="/bundles/smartbanner"></script>


    <script>
        $(document).ready(function() {
            $('.load-more').click(loadMoreEvents);
        });
        var page = 0,
            elemsPerPage = 25,
            loading = false;

        function loadMoreEvents() {
            if (!loading) {
                loading = true;
                $.post(
                    '/Organizations/5427/LoadMoreEvents',
                    {
                        id: 5427,
                        count: elemsPerPage,
                        offset: (++page) * elemsPerPage
                    }
                ).done(function(data) {
                    if (data) {
                        data = data.trim();
                        $('.load-more').before(data);
                    } else {
                        $('.load-more').hide();
                    }
                    loading = false;
                });
            }
        }
    </script>

