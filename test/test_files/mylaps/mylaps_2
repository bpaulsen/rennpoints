    });
  });
  $(".allresults-tab, .byclass-tab, .lapchartTab, .compare-tab").on('click', function () {
    var mode = $(this).attr("aria-controls");
    ga('send', {
      hitType: 'event',
      eventCategory: 'Event Results',
      eventAction: 'Session Tabs',
      eventLabel: mode || "all"
    });
  });
</script>
