  <script src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.js"></script>
<script type='text/javascript'>
  window.addEventListener("load", function () {
    window.cookieconsent.initialise({
      "palette": {
        "popup": {
          "background": "#e4e4e4",
          "text": "#20252c"
        },
        "button": {
          "background": "#50c7cd",
          "text": "#20252c"
        }
      },
      "type": "opt-out",
      "content": {
        "message": "This website uses cookies to ensure you get the best experience on our website.",
        "dismiss": "Accept",
        "deny": "Decline",
        "link": "Learn more",
        "href": "https://speedhive.mylaps.com/cookiepolicy"
      },
      onPopupOpen: function (status) {
        // before option has been selected
        console.log('onPopupOpen');
        var type = this.options.type;
        var didConsent = this.hasConsented();
        if (type == 'opt-in' && didConsent) {
          disableCookies();
        }
        if (type == 'opt-out') {
          // enable cookies until disabled
          useCookies();
        }
      },
      onInitialise: function (status) {
        // after option has been selected
        console.log('onInitialise');
        var type = this.options.type;
        var didConsent = this.hasConsented();
        if (type == 'opt-in' && didConsent) {
          useCookies();
        }
        if (type == 'opt-out') {
          if (!didConsent) {
            disableCookies();
          } else {
            useCookies();
          }
        }
      },

      onStatusChange: function (status, chosenBefore) {
        var type = this.options.type;
        var didConsent = this.hasConsented();
        if (type == 'opt-in' && didConsent) {
          useCookies();
        }
        if (type == 'opt-out' && !didConsent) {
          disableCookies();
        }
      },

      onRevokeChoice: function () {
        var type = this.options.type;
        if (type == 'opt-in') {
          disableCookies();
        }
        if (type == 'opt-out') {
          useCookies();
        }
      }
    });
  });
  function useCookies() {
    console.log('use cookies');
    ga('create', 'UA-886036-34');
    ga('send', 'pageview');

    /* <![CDATA[ */
    var google_conversion_id = 866216927;
    var google_custom_params = window.google_tag_params;
    var google_remarketing_only = true;
    /* ]]> */

    var googleAdsScript = document.createElement("script");
    googleAdsScript.src = "//www.googleadservices.com/pagead/conversion.js";
    document.head.appendChild(googleAdsScript);
  }
  function disableCookies() {
    console.log('disable cookies');
  }
</script>
  
    <script>
        $(document).ready(function() {
            $('.load-more').click(loadMoreEvents);
        });
        var page = 0,
            elemsPerPage = 25,
            loading = false;

        function loadMoreEvents() {
            if (!loading) {
                loading = true;
                $.post(
                    '/Organizations/5427/LoadMoreEvents',
                    {   id : 5427,
                        count : elemsPerPage,
                        offset: (++page)*elemsPerPage
                    }
                ).done(function(data){
                    if (data) {
                        data = data.trim();
                        $('.load-more').before(data);
                    } else {
                        $('.load-more').hide();
                    }
                    loading = false;
                });   
            }
        }
    </script>

